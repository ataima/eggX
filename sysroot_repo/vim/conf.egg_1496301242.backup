<?xml version="1.0"?>
<egg>
  <project>
    <name>vim</name>
    <download>check</download>
    <remote>
      <method>wget</method>
      <url>ftp://ftp.vim.org/pub/vim/unix</url>
      <file>vim-7.4.tar.bz2</file>
    </remote>
    <build>
      <step id="1">
        <silent>yes</silent>
        <threads>8</threads>
        <configure>
          <extra id="0">--prefix=$DEPLOY</extra>
          <extra id="1">--target=$CROSS</extra>
        </configure>
        <make id="0">
          <priority>1000</priority>
          <rule id="0">
            <name>all</name>
            <thread>1</thread>
          </rule>
          <rule id="1">
            <name>install-strip</name>
            <thread>1</thread>
          </rule>
        </make>
      </step>
    </build>
  </project>
</egg>
